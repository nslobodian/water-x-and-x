{
    "name": "development",
    "type": "postgres",
    "host": "localhost",
    "port": 5432,
    "username": "user",
    "password": "password",
    "database": "water",
    "synchronize": false,
    "logging": true,
    "entities": [
        "src/**/*.entity.ts",
        "dist/**/*.entity.js"
    ],
    "migrations": [
        "src/migration/**/*.ts",
        "dist/migration/**/*.js"
    ],
    "subscribers": [
        "src/subscriber/**/*.ts",
        "dist/subscriber/**/*.js"
    ],
    "cli": {
        "entitiesDir": "src",
        "migrationsDir": "src/migration",
        "subscribersDir": "src/subscriber"
    }
}